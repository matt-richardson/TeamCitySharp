    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <AssemblyName>Octopus.TeamCitySharp</AssemblyName>
    <Company>Octopus Deploy</Company>
    <Product>Octopus TeamCity Sdk</Product>
    <Copyright>Copyright Â© Octopus Deploy 2024</Copyright>
    <Authors>Octopus Deploy Pty Ltd</Authors>
    <PackageProjectUrl>https://github.com/OctopusDeploy/Octopus.TeamCitySharp</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <Description>A fork of https://github.com/mavezeau/TeamCitySharp.</Description>
    
    <!-- ignore obsolete RestSharp methods; see https://github.com/restsharp/RestSharp/pull/2228 -->
