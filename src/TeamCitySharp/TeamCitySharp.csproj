    <RootNamespace>Octopus.OnePassword.Sdk</RootNamespace>
   
  </PropertyGroup>
  <PropertyGroup>
    <DefineConstants>NETCORE;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
